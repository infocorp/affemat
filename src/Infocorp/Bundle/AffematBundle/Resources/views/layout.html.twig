<!DOCTYPE html>
<html lang="pt_BR">
<head>
  <title>{% block title %}Affemat{% endblock %}</title>
  {% block stylesheets %}
    {% stylesheets 
      'bundles/infocorpaffemat/css/bootstrap/bootstrap.min.css'
      'bundles/infocorpaffemat/css/bootstrap/bootstrap-responsive.css'
      filter='cssrewrite' 
    %}
      <link rel="stylesheet" href="{{ asset_url }}">
    {% endstylesheets %}
  {% endblock %}

  {% block javascripts %}
    {% javascripts 
      'bundles/infocorpaffemat/js/bootstrap.min.js'
    %}
      <script src="//ajax.googleapis.com/ajax/libs/jquery/1.8.2/jquery.min.js"></script>
      <script src="{{ asset_url }}"></script>
    {% endjavascripts %}
  {% endblock %}
</head>
<body>
  {% include 'InfocorpAffematBundle:Block:slider.html.twig' %}

  {% block content %}
  {% endblock %}
</body>
</html>